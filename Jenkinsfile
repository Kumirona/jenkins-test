pipeline {
    agent { docker { image 'maven:3.3.3' } }

    stages {
        stage('Build') {
            steps {
                bat 'mvn --version'
            }
        }
        stage('Test') {
            steps {
                bat 'mvn --version'
            }
        }
        stage('Deploy') {
            steps {
                bat 'mvn --version'
            }
        }
    }
}
